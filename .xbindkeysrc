# Volume keys
"pamixer --decrease 5"
  XF86AudioLowerVolume

"pamixer --increase 5"
  XF86AudioRaiseVolume

"pamixer --toggle-mute"
  XF86AudioMute

"playerctl play-pause"
  XF86AudioPlay

"playerctl next"
  XF86AudioNext

"playerctl previous"
  XF86AudioPrev

# Brightness keys
"xbacklight -dec 5"
  XF86MonBrightnessDown

"xbacklight -inc 5"
  XF86MonBrightnessUp
